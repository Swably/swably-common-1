<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
	<style name="mainMenu">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:background">@color/header</item>
	</style>
	<style name="mainMenuBackBar">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_alignParentRight">true</item>
	</style>
    <style name="titleBar">
		<item name="android:orientation">horizontal</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">48dp</item>
		<item name="android:background">@color/header</item>
		<item name="android:gravity">left|center_vertical</item>
	</style>
	<style name="titleBtn">
		<item name="android:layout_width">48dp</item>
		<item name="android:layout_height">48dp</item>
		<item name="android:background">@drawable/icon_clickable</item>
		<item name="android:clickable">true</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="titleBtn.wide">
		<!-- <item name="android:layout_width">56dp</item> -->
	</style>
	<style name="titleText">
		<item name="android:textSize">20sp</item>
		<item name="android:textColor">@color/title</item>
		<item name="android:singleLine">true</item>
		<item name="android:drawablePadding">5dp</item>
		<item name="android:shadowColor">#a0000000</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">1</item>
	</style>
	<style name="title" parent="titleText">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_weight">1</item>
		<item name="android:paddingLeft">14dp</item>
		<item name="android:paddingRight">10dp</item>
		<item name="android:clickable">true</item>
		<item name="android:gravity">center</item>
		<item name="android:textAllCaps">true</item>
	</style>
	<style name="title.btn">
		<item name="android:layout_height">48dp</item>
		<item name="android:background">@drawable/icon_clickable</item>
		<item name="android:gravity">left|center_vertical</item>
		<item name="android:clickable">true</item>
	</style>
	<style name="bottomBar">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">48dp</item>
		<item name="android:background">@color/header</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="bottomBtn">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_weight">1</item>
		<item name="android:background">@drawable/action_row</item>
		<item name="android:padding">@dimen/padding</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/back</item>
		<item name="android:gravity">center</item>
		<item name="android:singleLine">true</item>
		<item name="android:clickable">true</item>
	</style>
	<style name="btn">
		<item name="android:background">@drawable/btn</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">44dp</item>
		<item name="android:layout_weight">1</item>
		<item name="android:textColor">@color/btn</item>
		<item name="android:textSize">20sp</item>
		<item name="android:gravity">left|center_vertical</item>
		<item name="android:paddingLeft">15dp</item>
		<item name="android:paddingRight">15dp</item>
		<item name="android:drawablePadding">10dp</item>
	</style>
	<style name="actionText">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">@color/action</item>
		<item name="android:drawablePadding">10dp</item>
	</style>
	<style name="action_row">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:minHeight">@dimen/action_row_min_height</item>
		<item name="android:layout_weight">1</item>
		<item name="android:background">@drawable/action_row</item>
		<item name="android:padding">@dimen/padding</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/action</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:drawablePadding">7dp</item>
		<item name="android:singleLine">true</item>
		<item name="android:clickable">true</item>
	</style>
	<style name="action_row.no_min">
		<item name="android:minHeight">0dp</item>
	</style>
	<style name="action_count">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">@dimen/action_row_min_height</item>
		<item name="android:layout_alignParentRight">true</item>
		<item name="android:layout_marginRight">10dp</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">@color/action_count</item>
	</style>
	<style name="action_divider">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">1px</item>
	</style>
	<style name="action_vertical_divider">
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_width">1px</item>
		<item name="android:layout_marginTop">5dp</item>
		<item name="android:layout_marginBottom">5dp</item>
		<item name="android:background">@color/divider</item>
	</style>
	<style name="page_title">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/gray</item>
	</style>
	<style name="field">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:paddingTop">5dp</item>
		<item name="android:paddingBottom">5dp</item>
		<item name="android:paddingLeft">10dp</item>
		<item name="android:paddingRight">10dp</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/field</item>
		<item name="android:textColorHint">#ababab</item>
		<item name="android:background">@drawable/edit_text_holo_light</item>
	</style>
	<style name="field_prompt">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textColor">@color/content</item>
		<item name="android:paddingLeft">10dip</item>
	</style>
	<style name="name">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/name</item>
    </style>
	<style name="name.shadow">
		<item name="android:shadowColor">#a0000000</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">1</item>
    </style>
		<style name="content">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/content</item>
	</style>
	<style name="content.grey">
		<item name="android:textColor">@color/desc</item>
	</style>
	<style name="bar_content">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">#679aab</item>
	</style>
	<style name="desc">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
	    <item name="android:textSize">14sp</item>
		<item name="android:textColor">@color/desc</item>
	</style>
	<style name="settings_row">
		<item name="android:orientation">vertical</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:minHeight">@dimen/action_row_min_height</item>
		<item name="android:background">@drawable/row</item>
		<item name="android:layout_marginLeft">@dimen/padding</item>
		<item name="android:layout_marginRight">@dimen/padding</item>
		<item name="android:paddingBottom">@dimen/padding</item>
		<item name="android:clickable">true</item>
	</style>
	<style name="settings_desc">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">@color/settings_desc</item>
	</style>
	<style name="settings_desc.action">
		<item name="android:paddingTop">@dimen/padding</item>
		<item name="android:paddingBottom">@dimen/padding</item>
		<item name="android:layout_marginLeft">@dimen/padding</item>
		<item name="android:layout_marginRight">@dimen/padding</item>
		<item name="android:clickable">true</item>
		<item name="android:background">@drawable/row</item>
	</style>
	<style name="comment">
		<item name="android:padding">10dip</item>
	</style>
	<style name="avatar">
		<item name="android:layout_width">52dp</item>
		<item name="android:layout_height">52dp</item>
		<item name="android:adjustViewBounds">true</item>
	</style>
	<style name="avatar.thread">
		<item name="android:layout_width">42dp</item>
		<item name="android:layout_height">42dp</item>
	</style>
	<style name="avatar.bordered">
		<item name="android:layout_width">64dp</item>
		<item name="android:layout_height">64dp</item>
	    <item name="android:padding">6dp</item>
	    <item name="android:background">@drawable/circle</item>
	</style>
	<style name="app_icon">
		<item name="android:layout_width">48dp</item>
		<item name="android:layout_height">48dp</item>
		<item name="android:adjustViewBounds">true</item>
	</style>
	<style name="icon">
		<item name="android:layout_width">32dp</item>
		<item name="android:layout_height">32dp</item>
		<item name="android:adjustViewBounds">true</item>
	</style>
	<style name="list">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_weight">1</item>
		<item name="android:headerDividersEnabled">true</item>
		<item name="android:footerDividersEnabled">false</item>
		<item name="android:dividerHeight">0px</item>
		<item name="android:divider">@color/divider</item>
		<item name="android:fadingEdgeLength">0sp</item>
		<item name="android:cacheColorHint">#00000000</item>
		<item name="android:background">@color/back</item>
		<item name="android:listSelector">@drawable/none</item>
		<item name="android:scrollbarStyle">outsideOverlay</item>
		<item name="android:fadeScrollbars">true</item>
	</style>
	<style name="list.review">
	</style>
	<style name="list.user">
	</style>
	<style name="list.app">
	</style>
	<style name="navi_icon">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:adjustViewBounds">false</item>
	</style>
	<style name="guide_btn">
		<item name="android:background">@drawable/guide_btn</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:textColor">@color/title</item>
		<item name="android:textSize">18sp</item>
		<item name="android:gravity">left|center_vertical</item>
		<item name="android:padding">10dp</item>
	</style>

	<style name="holoBtn">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:gravity">center</item>
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">@color/back</item>
		<item name="android:background">@drawable/btn_default_holo_light</item>
		<item name="android:paddingLeft">20dp</item>
		<item name="android:paddingRight">20dp</item>
	</style>
	<style name="holoBtn.default">
	</style>
	<style name="holoBtn.blue">
		<item name="android:background">@drawable/btn_blue_holo</item>
	</style>
	<style name="holoBtn.blue.big">
		<item name="android:layout_height">48dp</item>
	    <item name="android:background">@drawable/btn_blue_big</item>
	    <item name="android:textSize">20sp</item>
		<item name="android:gravity">center</item>
		<item name="android:textColor">@color/title</item>
	</style>
	<style name="listLoading">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">#5c5c5c</item>
	</style>
	<style name="arrow_row_icon">
		<item name="android:layout_width">36dp</item>
		<item name="android:layout_height">36dp</item>
		<item name="android:layout_marginRight">10dp</item>
	</style>
	<style name="arrow_row_text_div">
		<item name="android:orientation">vertical</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:duplicateParentState">true</item>
	</style>
	<style name="arrow_row_title">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:singleLine">true</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/name</item>
		<item name="android:duplicateParentState">true</item>
	</style>
	<style name="arrow_row_desc">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:singleLine">true</item>
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">@color/desc</item>
	</style>
	<style name="tip_bubble">
		<item name="android:orientation">vertical</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:padding">@dimen/padding</item>
	</style>
	
    <style name="TabWidget">
   		<item name="android:layout_width">fill_parent</item>
   		<item name="android:layout_height">32dp</item>
		<item name="android:background">#222222</item>
   		<item name="android:ellipsize">marquee</item>
        <item name="android:textAppearance">@android:style/TextAppearance.Widget.TabWidget</item>
        <item name="android:divider">@null</item>
    </style>
    <style name="TabText">
        <item name="android:maxWidth">180dip</item>
        <item name="android:textAppearance">@android:style/TextAppearance.Medium</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">12sp</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="Tab">
        <item name="android:background">@drawable/tab_indicator</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="TabStrip">
   		<item name="android:layout_width">fill_parent</item>
   		<item name="android:layout_height">32dp</item>
   		<item name="android:layout_gravity">top</item>
   		<item name="android:gravity">center</item>
   		<item name="android:background">@color/tab_strip_back</item>
   		<item name="android:textColor">#fff</item>
   		<item name="android:textSize">12sp</item>
    </style>
    
    <style name="fill">
   		<item name="android:layout_width">fill_parent</item>
   		<item name="android:layout_height">fill_parent</item>
   		<item name="android:layout_weight">1</item>
   	</style>
    <style name="wrap">
   		<item name="android:layout_width">wrap_content</item>
   		<item name="android:layout_height">wrap_content</item>
   	</style>
    
	<style name="menu_row">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:singleLine">true</item>
		<item name="android:textSize">20sp</item>
		<item name="android:textColor">@color/hover_blue</item>
		<item name="android:padding">10dp</item>
		<item name="android:drawablePadding">10dp</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:clickable">true</item>
   		<item name="android:background">@drawable/action_row</item>
		<item name="android:paddingLeft">15dp</item>
		<item name="android:paddingRight">15dp</item>
	</style>
	
    <style name="divider">
   		<item name="android:orientation">vertical</item>
        <item name="android:layout_width">fill_parent</item>
   		<item name="android:layout_height">1dp</item>
   		<item name="android:background">@color/divider</item>
	</style>
	<style name="btnTriangle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:src">@drawable/btn_triangle</item>
		<item name="android:clickable">true</item>
		<item name="android:layout_alignParentRight">true</item>
		<item name="android:paddingBottom">@dimen/padding</item>
		<item name="android:paddingRight">@dimen/padding</item>
   	</style>
    <style name="inplacePanel">
   		<item name="android:layout_width">fill_parent</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:background">@color/header</item>
   	</style>
    <style name="inplaceBtn">
   		<item name="android:layout_width">fill_parent</item>
   		<item name="android:layout_height">fill_parent</item>
   		<item name="android:layout_weight">1</item>
		<item name="android:gravity">center</item>
   		<item name="android:background">@drawable/action_row</item>
		<item name="android:clickable">true</item>
		<item name="android:focusable">false</item>
		<item name="android:textColor">@color/hover_blue</item>
		<item name="android:textSize">18sp</item>
	</style>
    <style name="inplaceBtn.shareButton">
   		<item name="android:layout_width">@dimen/shareButtonSize</item>
   		<item name="android:layout_height">@dimen/shareButtonSize</item>
		<item name="android:paddingBottom">@dimen/shareButtonPadding</item>
		<item name="android:paddingTop">@dimen/shareButtonPadding</item>
   	</style>
    <style name="sectionTitle">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/panel</item>
		<item name="android:paddingTop">6dp</item>
		<item name="android:paddingBottom">6dp</item>
	</style>
	<style name="sectionBar">
		<item name="android:orientation">vertical</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:background">@color/back</item>
		<item name="android:gravity">left</item>
		<item name="android:paddingLeft">10dp</item>
		<item name="android:paddingRight">10dp</item>
	</style>
	<style name="sectionBar.topPadding">
		<item name="android:paddingTop">10dp</item>
	</style>
	
    <style name="ProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/progress_indeterminate_horizontal_holo</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">6dp</item>
        <item name="android:indeterminate">true</item>
    </style>
	
	<style name="addBtn">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:background">@color/none</item>
		<item name="android:src">@drawable/btn_add_right</item>
		<item name="android:clickable">true</item>
		<item name="android:gravity">right</item>
	</style>
	<style name="roundBtnLight">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:minWidth">22dp</item>
		<item name="android:layout_height">22dp</item>
		<item name="android:background">@drawable/round_btn_light</item>
		<item name="android:clickable">true</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="roundBtnLight.text">
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/panel</item>
	    <item name="android:paddingLeft">@dimen/padding</item>
		<item name="android:paddingRight">@dimen/padding</item>
	</style>
	<style name="roundBtnLight.text.light">
		<item name="android:textColor">@color/back</item>
	</style>
</resources>